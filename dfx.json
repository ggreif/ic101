{
  "canisters": {
    "ic101": {
      "main": "src/ic101/main.mo",
      "type": "motoko"
    },
    "ic101_assets": {
      "dependencies": [
        "ic101"
      ],
      "frontend": {
        "entrypoint": "src/ic101_assets/public/index.js"
      },
      "source": [
        "src/ic101_assets/assets",
        "dist/ic101_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.6.2",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "tungsten": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}